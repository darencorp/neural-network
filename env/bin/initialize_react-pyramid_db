#!/Users/darencorp/Documents/Projects/Python/react-pyramid-example/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'react-pyramid','console_scripts','initialize_react-pyramid_db'
__requires__ = 'react-pyramid'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('react-pyramid', 'console_scripts', 'initialize_react-pyramid_db')()
    )
